					window.quantity_rules["{{ key }}"] = {{ meta_object | json }};
				{%- endif -%}
			{%- endfor -%}
			{% comment %} qty_rules.forEach((qty_rule) => {
				const key = qty_rule.product_type.toLowerCase();
				window.quantity_rules[key] = qty_rule
			}); {% endcomment %}
		</script>		

